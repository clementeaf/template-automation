                    "dynamodb:Scan",
                    "dynamodb:GetItem",
                    "dynamodb:PutItem",
                    "dynamodb:UpdateItem",
                    "dynamodb:DeleteItem",
                    "dynamodb:BatchGetItem",
                    "dynamodb:BatchWriteItem"
                  ],
                  "Resource": [
                    "arn:aws:dynamodb:us-east-1:*:table/backend-serverless-dev",
                    "arn:aws:dynamodb:us-east-1:*:table/backend-serverless-dev/index/*"
